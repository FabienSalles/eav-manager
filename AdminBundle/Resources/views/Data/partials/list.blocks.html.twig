{% block list_modal %}
    {% use '@CleverAgeEAVManagerAdmin/Base/partials/list.blocks.html.twig' with list_modal as parent_list_modal %}
    {{ block('parent_list_modal') }}
{% endblock %}

{% block datagrid %}
    {% import "SidusDataGridBundle:DataGrid:datagrid.html.twig" as DataGridHelper %}
    {{ DataGridHelper.render(datagrid, {family: family, admin: admin}) }}
{% endblock %}

{% block list_title %}
    {{ tryTrans([admin_trans_key ~ '.title', 'admin.list_of'], {'%family%': family|trim}) }}
{% endblock %}
